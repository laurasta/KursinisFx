<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="182.0" prefWidth="383.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.kursinisfx.fxControllers.NewForumWindow">
   <children>
      <MenuBar prefHeight="25.0" prefWidth="382.0">
         <menus>
            <Menu mnemonicParsing="false" text="Back">
               <items>
                  <MenuItem fx:id="cancelForumCreate" mnemonicParsing="false" onAction="#returnToPrevious" text="Go back" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <Label layoutX="142.0" layoutY="25.0" text="Create new forum">
         <font>
            <Font name="JetBrains Mono ExtraBold" size="15.0" />
         </font></Label>
      <TextField fx:id="titleField" layoutX="33.0" layoutY="62.0" promptText="Enter title" />
      <TextField fx:id="descriptionField" layoutX="33.0" layoutY="111.0" promptText="Enter description" />
      <Label layoutX="33.0" layoutY="45.0" text="Title:" />
      <Label layoutX="32.0" layoutY="94.0" text="Description:" />
      <Button fx:id="createForumF" layoutX="300.0" layoutY="111.0" mnemonicParsing="false" onAction="#CreateForum" text="Create" />
   </children>
</AnchorPane>
