<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="205.0" prefWidth="183.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.kursinisfx.fxControllers.LoginPage">
   <children>
      <TextField fx:id="loginField" layoutX="17.0" layoutY="35.0" promptText="Enter login" />
      <PasswordField fx:id="passwordField" layoutX="17.0" layoutY="90.0" promptText="Enter password" />
      <Button layoutX="15.0" layoutY="166.0" mnemonicParsing="false" onAction="#validate" text="Log in" />
      <Button layoutX="112.0" layoutY="166.0" mnemonicParsing="false" onAction="#openRegistration" text="Sign up" />
      <Label layoutX="17.0" layoutY="18.0" text="Login:" />
      <Label layoutX="17.0" layoutY="73.0" text="Password:" />
   </children>
</AnchorPane>
